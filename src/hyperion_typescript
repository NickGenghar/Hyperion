import { HyperionClient } from "./hyperion_typescript";

declare module 'hyperion' {
    namespace HyperionClient {
        type options = {
            developers?: string[];
        };
        type HyperionOptions = object;
    }
    namespace BaseEvent {
        type name = string;
        type client = HyperionClient;
        type entries = string | string[];
    }
    namespace BaseCommand {
        type name = string;
        type options = {
            locale?: string;
            alias?: string[];
            desc?: string;
            usage?: string[];
            dev?: boolean;
            mod?: boolean;
            activate?: boolean;
        }
    }
}